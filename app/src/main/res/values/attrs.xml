<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BorderTextView">
        <attr name="borderWidth" format="dimension" />
        <attr name="rotation" format="dimension" />
        <attr name="borderColor" format="color" />
        <attr name="followTextColor" format="boolean" />
    </declare-styleable>

    <declare-styleable name="UsageRateProgress">
        <attr name="progress" format="integer" />
        <attr name="progressName" format="string" />
        <attr name="nameTextColor" format="color|reference" />
        <attr name="progressResidueColor" format="color|reference" />
        <attr name="progressColor" format="color|reference" />
        <attr name="insideCircleColor" format="color|reference" />
        <attr name="openAnimotion" format="boolean" />
    </declare-styleable>
</resources>